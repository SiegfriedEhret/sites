{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", ".vercel_build_output/**", ".svelte-kit/**"],
      "inputs": ["apps/src/**/*.{html,js,svelte,svx,ts}", "packages/**/*.{html,js,svelte,svx,ts}" ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
