{
  "name": "sites",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "turbo run build",
    "build:ehretdotme": "turbo run build --scope ehretdotme",
    "build:meloedotfr": "turbo run build --scope meloedotfr",
    "build:siegdotfr": "turbo run build --scope siegdotfr",
    "dev": "turbo run dev --parallel",
    "tina": "turbo run tina --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "turbo": "^1.13.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.1.2"
}