{
  "name": "sites",
  "version": "1.0.0",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "turbo run build",
    "build:ehretdotme": "turbo run build --filter=ehretdotme",
    "build:meloedotfr": "turbo run build --filter=meloedotfr",
    "build:siegdotfr": "turbo run build --filter=siegdotfr",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^2.8.8",
    "turbo": "^2.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.2.0"
}